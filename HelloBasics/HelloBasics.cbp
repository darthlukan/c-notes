<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="HelloBasics" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option show_notes="1">
			<notes>
				<![CDATA[A collection of things that I found to be of note when learning C.  This program is designed to help practice basic concepts and to serve as a "live" notes setup.]]>
			</notes>
		</Option>
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/HelloBasics" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/HelloBasics" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
